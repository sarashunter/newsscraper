<div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 d-flex pb-3">
    <div class=" card card-block">
    <div class="card-header"><h4><a href="{{this.url}}" class="articleTitle">{{this.title}}</a></h4>
    </div>
    <div class="card-body d-flex flex-column">
        <div class="card-subtitle text-muted text-left">By: {{this.author}}</div>
        <div class="mt-auto text-right"><a href="scraped/{{this.id}}" class="articleButton btn btn-light"><img src="/assets/open-iconic-master/svg/comment-square.svg" alt="icon name"></a></div>
    </div>
</div>
</div>