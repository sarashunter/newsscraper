<div class="col-article-box col-xs-12 col-sm-6 col-md-4 col-lg-3 card">
    <div class="card-title"><a href="{{this.url}}">{{this.title}}</a>
    </div>
    <div class="card-body">
        <div class="card-subtitle text-muted">By: {{this.author}}</div>
        <a href="scraped/{{this.id}}" class="articlebutton btn btn-primary">{{this.comments.length}} Comments</a>
    </div>
</div>